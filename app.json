{
  "expo": {
    "name": "FlashCards",
    "slug": "FlashCards",
    "description": "A Flashcards app",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.udacityflashcards.exp",
      "supportsTablet": true
    },
    "android": {
      "package": "org.udacityflashcards.exp"
    }
  }
}
